<h4>Files</h4>
<hr />

<table class="table table-striped">
  <thead>
  <tr>
    <th>File Name</th>
    <th>Downloads</th>
  </tr>
  </thead>
  <tbody>
    <% @top_downloads.each do |download| %>
      <% file = FileSet.find(download[0]) rescue file = nil %>
      <% if file %>
        <tr>
          <td><%= file %></td>
          <td><%= download[1] %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>