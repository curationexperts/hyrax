<h4>Works</h4>
<hr />

<table class="table table-striped">
  <thead>
    <tr>
      <th>Work Title</th>
      <th>Views</th>
    </tr>
  </thead>
  <tbody>
    <% @top_works.each do |work| %>
      <tr>
        <td><%= link_to work[2], admin_analytics_work_report_path(work[0]) %></td>
        <td><%= work[1] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="pull-right">
  <%= paginate @top_works %>
</div>
