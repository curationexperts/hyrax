<table class="table table-striped">
<%# TODO: Dynamic %>
  <thead>
  <tr>
    <th>&nbsp;</th>
    <th>Date Range</th>
    <th>Today</th>
    <th>This week</th>
    <th>This month</th>
    <th>This year</th>
    <th>All time</th>
  </tr>
  </thead>
  <tbody>
    <tr>
      <td><b>Downloads</b></td>
      <td><%#= today %></td>
      <td><%#= this_week %></td>
      <td><%#= this_month %></td>
      <td><%#= this_year %></td>
      <td><%#= all_time %></td>
    </tr>
    <tr>
      <td><b>Views</b></td>
      <td><%= Hyrax::Analytics.collections_pageviews("range", "#{@start_date},#{@end_date}") %></td>
      <td><%= Hyrax::Analytics.collections_pageviews("day") %></td>
      <td><%= Hyrax::Analytics.collections_pageviews("week") %></td>
      <td><%= Hyrax::Analytics.collections_pageviews("month") %></td>
      <td><%= Hyrax::Analytics.collections_pageviews("year") %></td>
      <td><%= Hyrax::Analytics.collections_pageviews("range", "#{Date.today-10.years},#{Date.today}") %></td>
    </tr>

  </tbody>
</table>